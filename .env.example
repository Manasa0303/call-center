# Plivo Credentials
# Get these from https://console.plivo.com/dashboard/
PLIVO_AUTH_ID=your_plivo_auth_id_here
PLIVO_AUTH_TOKEN=your_plivo_auth_token_here

# Phone Numbers (E.164 format: +country_code + number, no spaces)
# Your Plivo phone number that will make the call
PLIVO_SOURCE_NUMBER=+1234567890

# Default target phone number to call (optional, can be set in UI)
TARGET_NUMBER=+1234567890
